{% if Order.payment_method == 'UnivaPay' %}
    <script>
        $(function () {
            // ページ最下部に移動
            $('#univapay-info').appendTo($('#form1 > div.c-contentsArea__cols > div > div'));
        });
    </script>
    {% if Order.univa_pay_charge_id %}
        {% include '@UnivaPay/admin/order/charge.twig' %}
    {% elseif Order.univa_pay_subscription_id %}
        {% include '@UnivaPay/admin/order/subscription.twig' %}
    {% endif %}
{% endif %}
